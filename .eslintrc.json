{
    "root": true,
    "extends": [
      "eslint:recommended", /* eslint recommended rules template */
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "browser": true,
      "node": true,
      "mocha": true
    },
    "rules": {
        "no-console": 1 /* 0 = off; 1 = Warning; 2 = Error */
  
    }
  }